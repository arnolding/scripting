	}
	var pre_b;
	if (pe == 'Week') {pre_b = pre_week;}
	if (pe == 'BiWeek') {pre_b = pre_2week;}
	if (pe == 'Month') {pre_b = pre_month;}
	if (pe == 'HalfYr') {pre_b = pre_halfyr;}
	if (pe == 'Year') {pre_b = pre_year;}
	return pre_b;
}

	
